{
  "name": "Med UERJ",
  "description": "Dicas de biologia e química para a prova da UERJ na sua página de nova aba.",
  "version": "0.2",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "src/pages/index.html"
  },
  "background": {
    "scripts": []
