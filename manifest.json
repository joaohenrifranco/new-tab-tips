{
  "name": "Dica Rápida",
  "description": "Uma dica para o vestibular a cada vez que você abre uma nova aba.",
  "version": "0.1",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "src/pages/index.html"
  },
  "background": {
    "scripts": []
  },
  "permissions": [
    "storage"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
